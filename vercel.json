{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/public"
      }
    },
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/security-scanner.ts",
      "use": "@vercel/node"
    },
    {
      "src": "api/debug.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.ts"
    },
    {
      "src": "/api/security-scanner",
      "dest": "/api/security-scanner.ts"
    },
    {
      "src": "/api/debug",
      "dest": "/api/debug.ts"
    },
    {
      "src": "/wordpress-maintenance-worker.zip",
      "dest": "/api/index.ts"
    },
    {
      "src": "/wp-remote-manager-.*\\.zip",
      "dest": "/api/index.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(dashboard|clients|websites|tasks|profile|subscription|login|register|client-reports|reports).*",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}